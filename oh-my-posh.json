{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {

          },
          "leading_diamond": "\ue0b6",
          "template": "{{ .Icon }} <b>{{ .HostName }}</b>",
          "foreground": "#00885A",
          "background": "#003543",
          "type": "os",
          "style": "diamond"
        },
        {
          "options": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": " \uebcb ",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "template": " <b>\uf07b  </b> <b>{{ .Path }}</b> ",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "powerline_symbol_color": "#ffffff",
          "background": "#DA627D",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "branch_icon": " \ue0a0 ",
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "template": " <b>➜</b> (<b>{{ .UpstreamIcon }}{{ .HEAD }}</b>{{ if gt .StashCount 0 }} <b>\uf692</b> <b>{{ .StashCount }}</b>{{ end }}) ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#FCA17D",
          "type": "git",
          "style": "powerline"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " <b>\ue718</b> {{ if .PackageManagerIcon }}<b>{{ .PackageManagerIcon }}</b> {{ end }}<b>{{ .Full }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#4a7c59",
          "type": "node",
          "style": "powerline"
        },
        {
          "options": {
            "always_enabled": true
          },
          "template": " <b>\ufbab {{ .FormattedMs }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#83769c",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "options": {
            "time_format": "15:04:05"
          },
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#33658A",
          "type": "time",
          "style": "powerline"
        },
        {
          "options": {
            "display_mode": "always",
            "home_enabled": true
          },
          "template": " <b>\ue718</b> {{ .Full }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#4a7c59",
          "type": "node",
          "style": "powerline"
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "always",
            "home_enabled": true
          },
          "template": " <b>\ue77f {{ .Full }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#6E8BDB",
          "type": "dotnet",
          "style": "powerline"
        },
       
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "always",
            "home_enabled": true
          },
          "template": " <b>\ue235 {{ .Full }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#3572A5",
          "type": "python",
          "style": "powerline"
        },
       {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc0000{{ end }}"
          ],
          "template": " <b>{{ if gt .Code 0 }}\uf525{{ else }}\uf469{{ end }}</b> ",
          "options": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "<b>\ue3bf</b> ",
          "foreground": "#cd5e42",
          "type": "root",
          "style": "plain"
        },
        {
          "options": {

          },
          "template": " <b>⚡</b> {{ .Name }} • {{ if .Version }}{{ .Version }}{{ end }}",
          "foreground": "#45F1C2",
          "type": "shell",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " | <b>{{ .UserName }}</b> \uf597 <b>❯</b> <b>❯</b>",
          "foreground": "#CD4277",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
