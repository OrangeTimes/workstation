{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {

          },
          "leading_diamond": "\ue0b6",
          "template": "<f size='2.0'>{{ .Icon }}</f> <b>{{ .HostName }}</>",
          "foreground": "#00885A",
          "background": "#003543",
          "type": "os",
          "style": "diamond"
        },
        {
          "options": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": " \uebcb ",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "template": " <fg=#000000><b><f size='1.5'>\uf07b \uf553</f></b></> <b>{{ .Path }}</b> ",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "powerline_symbol_color": "#ffffff",
          "background": "#DA627D",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "branch_icon": " <#ffffff><f size='1.2'>\ue0a0</f> </>",
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "template": " <b><f size='1.2'>➜</f></b> (<b>{{ .UpstreamIcon }}{{ .HEAD }}</b>{{ if gt .StashCount 0 }} <f size='1.2'>\uf692</f> <b>{{ .StashCount }}</b>{{ end }}) ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#FCA17D",
          "type": "git",
          "style": "powerline"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " <b><f size='2.0'>\ue718</f></b> {{ if .PackageManagerIcon }}<b>{{ .PackageManagerIcon }}</b> {{ end }}<b>{{ .Full }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#4a7c59",
          "type": "node",
          "style": "powerline"
        },
        {
          "options": {
            "always_enabled": true
          },
          "template": " <b><f size='1.2'>\ufbab</f> {{ .FormattedMs }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#83769c",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "options": {
            "time_format": "15:04:05"
          },
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#33658A",
          "type": "time",
          "style": "powerline"
        },
        {
          "options": {
            "display_mode": "always",
            "home_enabled": true
          },
          "template": " <b><f size='2.0'>\ue718</f></b> {{ .Full }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#4a7c59",
          "type": "node",
          "style": "powerline"
        },
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "always",
            "home_enabled": true
          },
          "template": " <b><f size='2.0'>\ue77f</f> {{ .Full }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#6E8BDB",
          "type": "dotnet",
          "style": "powerline"
        },
       
        {
          "options": {
            "cache_duration": "none",
            "display_mode": "always",
            "home_enabled": true
          },
          "template": " <b><f size='2.0'>\ue235</f> {{ .Full }}</b> ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#3572A5",
          "type": "python",
          "style": "powerline"
        },
       {
          "type": "status",
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#2e9599",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc0000{{ end }}"
          ],
          "template": " <b><f size='1.2'>{{ if gt .Code 0 }}\uf525{{ else }}\uf469{{ end }}</f></b> ",
          "options": {
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "<b><f size='1.3'>\ue3bf</f></b> ",
          "foreground": "#cd5e42",
          "type": "root",
          "style": "plain"
        },
        {
          "options": {

          },
          "template": " <#FFD700><b><f size='1.2'>⚡</f></b></><#45F1C2>{{ .Name }} • {{ if .Version }}{{ .Version }}{{ end }}</>",
          "foreground": "#45F1C2",
          "type": "shell",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": " | <#FF69B4><b>{{ .UserName }}</b></> <f size='1.2'>\uf597</f> <#26C6DA><b><f size='1.2'>❯</f></b></><#45F1C2><b><f size='1.2'>❯</f></b></>",
          "foreground": "#CD4277",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
